[application]

config/name="Politburo"
run/main_scene="res://GUI/Scenes/Title/MainMenu.tscn"
config/icon="res://Images/GUI/icon.png"

[autoload]

Global="*res://GlobalScripts/Global.gd"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/dialogic/plugin.cfg", "res://addons/folder_node/plugin.cfg" )

[importer_defaults]

texture={
